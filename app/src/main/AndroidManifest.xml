<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hamid_pc.parkingbookingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:supportsRtl">
        <activity android:name=".UserActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserDetailActivity"></activity>
        <activity android:name=".UserFeedbackActivity"></activity>
        <activity android:name=".UserNewBookingActivity"></activity>
        <activity android:name=".UserBookingListActivity"></activity>
        <activity android:name=".UserBookingEntryActivity"></activity>

    </application>

</manifest>
