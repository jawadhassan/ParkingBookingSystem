<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hamid_pc.parkingbookingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:supportsRtl">
        <activity
            android:name=".AuthenticationActivity"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserDetailActivity"></activity>
        <activity android:name=".UserFeedbackActivity"></activity>
        <activity android:name=".UserPlotListActivity"></activity>
        <activity android:name=".UserBookingListActivity"></activity>
        <activity android:name=".UserBookingEntryActivity"></activity>
        <activity android:name=".UserActivity"></activity>
        <activity android:name=".AdminActivity"></activity>
        <activity android:name=".AdminPlotListActivity"></activity>
        <activity android:name=".AddSlotActivity"></activity>
        <activity android:name=".AdminUserDetailActivity"></activity>
        <activity android:name=".AdminUserListActivity"></activity>
        <activity android:name=".AdminBookingListActivity"></activity>
        <activity android:name=".BookingDetailActivity"></activity>
        <activity android:name=".AdminFeedbackActivity"></activity>
        <activity
            android:name=".AddPlotActivity"
            android:windowSoftInputMode="adjustResize"></activity>

    </application>

</manifest>
